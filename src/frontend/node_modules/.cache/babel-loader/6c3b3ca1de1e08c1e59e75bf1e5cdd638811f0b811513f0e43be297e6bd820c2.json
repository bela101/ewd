{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { login } from '@/services/auth.service';\nexport default {\n  data: () => ({\n    username: '',\n    password: ''\n  }),\n  methods: {\n    submit() {\n      login({\n        username: this.username,\n        password: this.password\n      }).then(() => this.$router.push('/')).catch(err => console.error(err));\n    }\n  }\n};","map":{"version":3,"names":["login","data","username","password","methods","submit","then","$router","push","catch","err","console","error"],"sources":["/Users/bela/Library/Mobile Documents/com~apple~CloudDocs/bht/ewd/ewd/src/frontend/src/components/LoginView.vue"],"sourcesContent":["<!--<template>-->\n<!--  <div>-->\n<!--    <h2>Login</h2>-->\n<!--    <form @submit.prevent=\"submit\">-->\n<!--      <input v-model=\"username\" placeholder=\"Username\" />-->\n<!--      <input type=\"password\" v-model=\"password\" placeholder=\"Password\" />-->\n<!--      <button>Sign In</button>-->\n<!--    </form>-->\n<!--    <div>No account? <router-link to=\"/register\">Register Here</router-link></div>-->\n\n<!--  </div>-->\n<!--</template>-->\n\n<script>\nimport { login } from '@/services/auth.service';\nexport default {\n  data: () => ({ username: '', password: '' }),\n  methods: {\n    submit() {\n      login({ username: this.username, password: this.password })\n          .then(() => this.$router.push('/'))\n          .catch(err => console.error(err));\n    }\n  }\n};\n</script>\n\n<template>\n  <div class=\"login-page\">\n    <!-- Left pane -->\n    <div class=\"login-page__brand\">\n      <!-- Replace with your actual logo -->\n      <img src=\"@/assets/logo.png\" alt=\"Verdant\" class=\"login-page__logo\" />\n      <h1 class=\"login-page__title\">Verdant</h1>\n    </div>\n\n    <!-- Right pane: the form -->\n    <div class=\"login-page__form\">\n      <h2 class=\"login-page__heading\">Log In</h2>\n      <form @submit.prevent=\"onSubmit\" class=\"form\">\n        <label class=\"form__label\">\n          Email\n          <input\n              type=\"email\"\n              v-model=\"email\"\n              required\n              placeholder=\"you@example.com\"\n              class=\"form__input\"\n          />\n        </label>\n\n        <label class=\"form__label form__label--password\">\n          Passwort\n          <div class=\"form__password-wrapper\">\n            <input\n                :type=\"showPassword ? 'text' : 'password'\"\n                v-model=\"password\"\n                required\n                placeholder=\"Enter your password\"\n                class=\"form__input\"\n            />\n            <button\n                type=\"button\"\n                aria-label=\"Toggle password visibility\"\n                class=\"form__toggle-password\"\n                @click=\"showPassword = !showPassword\"\n            >\n              <!-- simple eye icon -->\n              <svg\n                  v-if=\"!showPassword\"\n                  xmlns=\"http://www.w3.org/2000/svg\"\n                  width=\"20\"\n                  height=\"20\"\n                  fill=\"currentColor\"\n                  viewBox=\"0 0 16 16\"\n              >\n                <path\n                    d=\"M16 8s-3-5.5-8-5.5S0 8 0 8s3 5.5 8 5.5S16 8 16 8z\"\n                />\n                <path\n                    d=\"M8 5a3 3 0 100 6 3 3 0 000-6z\"\n                    fill=\"#fff\"\n                />\n              </svg>\n              <svg\n                  v-else\n                  xmlns=\"http://www.w3.org/2000/svg\"\n                  width=\"20\"\n                  height=\"20\"\n                  fill=\"currentColor\"\n                  viewBox=\"0 0 16 16\"\n              >\n                <path\n                    d=\"M13.359 11.238l1.387 1.387L15 .761l-.762-.762-1.387 1.387a8.02 8.02 0 01-4.61 1.374C3 2.76 0 8 0 8s1.558 2.848 3.937 4.453l-1.387 1.387L3.537 15l1.387-1.387A8.02 8.02 0 018 14c5 0 8-5.5 8-5.5s-1.163-2.239-2.641-3.762zm-5.36-3.6a3 3 0 014.242 4.242L8.999 7.639z\"\n                />\n              </svg>\n            </button>\n          </div>\n        </label>\n\n        <button type=\"submit\" class=\"form__submit\">\n          Log In\n        </button>\n      </form>\n\n      <p class=\"login-page__toggle\">\n        Noch keinen Account?\n        <router-link to=\"/register\">Account erstellen</router-link>\n      </p>\n    </div>\n  </div>\n</template>\n\n<style scoped>\n.login-page {\n  display: flex;\n  min-height: 100vh;\n}\n\n/* Left brand panel */\n.login-page__brand {\n  flex: 1;\n  background: linear-gradient(135deg, #f0fff4 0%, #d9f5e0 100%);\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n  justify-content: center;\n}\n\n.login-page__logo {\n  max-width: 120px;\n  margin-bottom: 1rem;\n}\n\n.login-page__title {\n  font-size: 2.5rem;\n  font-weight: 600;\n  color: #4caf50;\n}\n\n/* Right form panel */\n.login-page__form {\n  flex: 1;\n  padding: 3rem;\n  display: flex;\n  flex-direction: column;\n  justify-content: center;\n}\n\n.login-page__heading {\n  font-size: 1.75rem;\n  font-weight: 500;\n  margin-bottom: 2rem;\n  color: #222;\n}\n\n/* Form styles */\n.form {\n  display: flex;\n  flex-direction: column;\n}\n\n.form__label {\n  font-size: 0.95rem;\n  margin-bottom: 1.25rem;\n  color: #333;\n}\n\n.form__input {\n  width: 100%;\n  padding: 0.75rem 1rem;\n  margin-top: 0.5rem;\n  border: 1px solid #d1d5db;\n  border-radius: 0.375rem;\n  font-size: 1rem;\n  outline: none;\n}\n\n.form__input:focus {\n  border-color: #4caf50;\n  box-shadow: 0 0 0 3px rgba(76, 175, 80, 0.2);\n}\n\n/* Password field + toggle */\n.form__label--password {\n  position: relative;\n}\n\n.form__password-wrapper {\n  position: relative;\n}\n\n.form__toggle-password {\n  position: absolute;\n  right: 0.75rem;\n  top: 50%;\n  transform: translateY(-50%);\n  background: none;\n  border: none;\n  padding: 0;\n  cursor: pointer;\n  color: #9ca3af;\n}\n\n/* Submit button */\n.form__submit {\n  margin-top: 1.5rem;\n  padding: 0.75rem;\n  font-size: 1rem;\n  font-weight: 500;\n  border: none;\n  border-radius: 0.375rem;\n  background-color: #2563eb;\n  color: white;\n  cursor: pointer;\n}\n\n.form__submit:hover {\n  background-color: #1e40af;\n}\n\n/* Bottom toggle text */\n.login-page__toggle {\n  margin-top: 1.5rem;\n  font-size: 0.9rem;\n  color: #6b7280;\n}\n\n.login-page__toggle a {\n  color: #2563eb;\n  text-decoration: none;\n}\n\n.login-page__toggle a:hover {\n  text-decoration: underline;\n}\n</style>\n"],"mappings":";AAcA,SAASA,KAAI,QAAS,yBAAyB;AAC/C,eAAe;EACbC,IAAI,EAAEA,CAAA,MAAO;IAAEC,QAAQ,EAAE,EAAE;IAAEC,QAAQ,EAAE;EAAG,CAAC,CAAC;EAC5CC,OAAO,EAAE;IACPC,MAAMA,CAAA,EAAG;MACPL,KAAK,CAAC;QAAEE,QAAQ,EAAE,IAAI,CAACA,QAAQ;QAAEC,QAAQ,EAAE,IAAI,CAACA;MAAS,CAAC,EACrDG,IAAI,CAAC,MAAM,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC,GAAG,CAAC,EACjCC,KAAK,CAACC,GAAE,IAAKC,OAAO,CAACC,KAAK,CAACF,GAAG,CAAC,CAAC;IACvC;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}